Problem Code: CANDY123 | Name: Bear and Candies 123 | Difficulty: 1028

#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin>>t;
    while(t--){
        int a,b;
        cin>>a>>b;
        for(int i=0;i<=1000;i++){
            if(i%2 != 0){
	            a-=i;
	            if(a<0){
	                cout<<"Bob"<<endl;
	                break;
	            }
	        }
	        else{
	            b-=i;
	            if(b<0){
	                cout<<"Limak"<<endl;
	                break;
	            }
	        }
        }
    }
	return 0;
}
